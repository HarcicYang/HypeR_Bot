{
  "categories": {
    "list": {
      "url": "https://api.bilibili.com/x/web-interface/web/channel/category/list",
      "method": "GET",
      "comment": "获取所有的一级频道"
    },
    "sub_channels": {
      "url": "https://api.bilibili.com/x/web-interface/web/channel/category/channel_arc/list",
      "method": "GET",
      "params": {
        "id": "int: 一级频道 id (与通用频道 id 不互通)",
        "offset": "str: 自行理解"
      }
    }
  },
  "channel": {
    "info": {
      "url": "tnnd, 这个数据放在了 html 里面！"
    },
    "list_featured": {
      "url": "https://api.bilibili.com/x/web-interface/web/channel/featured/list",
      "method": "GET",
      "params": {
        "channel_id": "int: 频道 id",
        "filter_type": "int: 年份，可以为空",
        "offset": "str: 自行理解",
        "page_size": "int: 数据大小"
      },
      "comment": "获取频道中的精选视频"
    },
    "list_multiple": {
      "url": "https://api.bilibili.com/x/web-interface/web/channel/multiple/list",
      "method": "GET",
      "params": {
        "channel_id": "int: 频道 id",
        "sort_type": "str: 排序方式。new 最新, hot 最火, view 播放量最高",
        "offset": "str: 自行理解",
        "page_size": "int: 数据大小"
      },
      "comment": "获取频道中的综合视频"
    },
    "subscribe": {
      "url": "https://api.bilibili.com/x/web-interface/web/channel/subscribe",
      "method": "POST",
      "verify": true,
      "data": {
        "id": "int: 频道 id"
      },
      "comment": "关注频道"
    },
    "unsubscribe": {
      "url": "https://api.bilibili.com/x/web-interface/web/channel/unsubscribe",
      "method": "POST",
      "verify": true,
      "data": {
        "id": "int: 频道 id"
      },
      "comment": "取关频道"
    }
  },
  "self_subscribes": {
    "list": {
      "url": "https://api.bilibili.com/x/web-interface/web/channel/subscribe/list",
      "method": "GET",
      "verify": true,
      "comment": "获取关注的频道"
    }
  }
}
